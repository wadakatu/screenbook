{
	"name": "@screenbook/cli",
	"version": "1.5.0",
	"description": "Screen catalog and navigation graph generator CLI. Auto-detect frameworks, generate docs, and analyze impact.",
	"type": "module",
	"bin": {
		"screenbook": "./dist/index.mjs"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.mts"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@apidevtools/swagger-parser": "^12.1.0",
		"@babel/parser": "^7.28.5",
		"@screenbook/core": "workspace:*",
		"@vue/compiler-sfc": "^3.5.0",
		"gunshi": "^0.27.0",
		"htmlparser2": "^9.1.0",
		"jiti": "^2.6.1",
		"minimatch": "^10.1.1",
		"picocolors": "^1.1.1",
		"prompts": "^2.4.2",
		"tinyglobby": "^0.2.0"
	},
	"devDependencies": {
		"@types/prompts": "^2.4.9",
		"tsdown": "catalog:"
	},
	"keywords": [
		"screenbook",
		"cli",
		"screen-catalog",
		"navigation-graph",
		"documentation",
		"documentation-generator",
		"frontend",
		"developer-tools",
		"impact-analysis",
		"typescript",
		"nextjs",
		"react",
		"vue"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/wadakatu/screenbook.git",
		"directory": "packages/cli"
	},
	"homepage": "https://github.com/wadakatu/screenbook#readme",
	"bugs": {
		"url": "https://github.com/wadakatu/screenbook/issues"
	}
}
